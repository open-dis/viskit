<?xml version="1.0" encoding="iso-8859-1" standalone="yes" ?>
<izpack:shortcuts version="5.0"
                  xmlns:izpack="http://izpack.org/schema/shortcuts"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://izpack.org/schema/shortcuts http://izpack.org/schema/5.0/izpack-shortcuts-5.0.xsd">
    
  <skipIfNotSupported/>
  
  <programGroup defaultName="Viskit\Viskit-1.0.16" location="applications"/>

  <shortcut
     name="Viskit"
     programGroup="yes"
     desktop="yes"
     applications="yes"
     startMenu="yes"
     startup="no"
     target="$INSTALL_PATH\Viskit.exe"
     commandLine=""
     description="This installs Viskit"
     iconFile="$INSTALL_PATH\images\ViskitIcon.ico"
     iconIndex="0"
     initialState="noShow">

     <createForPack name="Core"/>
   </shortcut>

   <shortcut
     name="Viskit-1.0.16 Uninstaller"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH\Uninstaller\uninstaller.jar"
     commandLine=""
     description="This uninstalls Viskit"
     iconFile="%SystemRoot%\system32\SHELL32.dll"
     iconIndex="31">

     <createForPack name="Core" />
   </shortcut>

</izpack:shortcuts>
