<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SimEntity name="ArrivalProcess" package="examples" version="21 May 2016" extend="SimEntityBase" description="Model of a simple recurring Arrival process" xsi:noNamespaceSchemaLocation="http://eos.nps.edu/Simkit/simkit.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Parameter name="interarrivalTime" type="simkit.random.RandomVariate" description="todo"/>
    <StateVariable name="numberArrivals" type="int" description=""/>
    <Event name="Run" description="">
        <StateTransition state="numberArrivals">
            <Assignment value="0"/>
        </StateTransition>
        <Schedule delay="interarrivalTime.generate()" event="Arrival" priority="DEFAULT"/>
        <Coordinate x="50.0" y="80.0"/>
    </Event>
    <Event name="Arrival" description="">
        <StateTransition state="numberArrivals">
            <Assignment value="numberArrivals + 1"/>
        </StateTransition>
        <Schedule delay="interarrivalTime.generate()" event="Arrival" priority="DEFAULT"/>
        <Coordinate x="260.0" y="80.0"/>
    </Event>
</SimEntity>
