<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SimEntity name="ArrivalProcess" package="examples" version="0.1" extend="SimEntityBase" description="Model of a simple recurring Arrival process" xsi:noNamespaceSchemaLocation="http://diana.nps.edu/Simkit/simkit.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Parameter name="interarrivalTime" type="simkit.random.RandomVariate"/>
    <StateVariable name="numberArrivals" type="int"/>
    <Event name="Run" description="">
        <StateTransition state="numberArrivals">
            <Assignment value="0"/>
        </StateTransition>
        <Coordinate x="50.0" y="70.0"/>
    </Event>
</SimEntity>
