<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SimEntity name="ArrayTester" package="examples" version="21 May 2016" author="Terry D. Norbraten" extend="SimEntityBase" description="Test Viskit's capability to create primative type arrays" xsi:noNamespaceSchemaLocation="http://eos.nps.edu/Simkit/simkit.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Parameter description="" name="param_0" type="int[]"/>
    <StateVariable description="" name="state_0" type="int[param_0.length]">
        <Comment></Comment>
    </StateVariable>
    <Event name="Run" description="Begin simulation">
        <Schedule delay="0.0" event="Init" condition="true" priority="DEFAULT">
            <Comment></Comment>
            <EdgeParameter value="0"/>
        </Schedule>
        <Code></Code>
        <Coordinate x="30.0" y="30.0"/>
    </Event>
    <Event name="Init" description="Initialize">
        <Comment></Comment>
        <Argument description="" name="i" type="int"/>
        <StateTransition state="state_0" index="i">
            <Assignment value="param_0[i]"/>
        </StateTransition>
        <Schedule delay="0.0" event="Init" condition="i &lt; (param_0.length - 1)" priority="DEFAULT">
            <Comment></Comment>
            <EdgeParameter value="i + 1"/>
        </Schedule>
        <Code></Code>
        <Coordinate x="180.0" y="30.0"/>
    </Event>
</SimEntity>
